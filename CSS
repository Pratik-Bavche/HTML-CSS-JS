#CSS
/* Class selector */
.red{
    background-color: red;
}

/* Id selector */
#green{
    background-color: green;
}

/* Child selector */
.red>p{
    background-color: blue;
}

/* Decendant Selector */
.red p{
    background-color: blue;
}

/* For all screen contents */
/* *{
    font-family: 'Times New Roman', Times, serif;
    padding: 0;
    margin: 0;
} */

/* selector-visited site */
a:visited{
    color: red;
}

/* Not visited site */
a:link{
    color: green;
}

a:active{
    background-color: black;
}
/* OVER THE TEXT/BUTTON */
a:hover{
    background-color: coral;
}
/* for first paragraph in multiple
p:first-child{
    background-color: aqua;
}*/

.box{
    background-color: red;
    padding: 10px;
    margin: 15px;
    border: 2px solid blue;
    height: 100px;
    box-sizing: border-box;
}
p{
    /* font-family: Arial, Helvetica, sans-serif;
    font-style:oblique; */
    text-indent: 30px; /*ADDING SPACE BEFORE TEXT*/
    
}
/*ALL FONT RELATED*/
h1{
    font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
    font-style: oblique;
    font-weight: 5000;
    text-decoration: underline;
    color: brown;
    line-height: 5;
    letter-spacing: 10px;
    text-transform:uppercase;
    text-decoration-color: black;
    text-decoration-style: dotted;
    text-indent: 30px;
    text-align: center;
}
h1:hover{
    color: blue;
}
div{
    /* background-color: blue; */
    color: black;
}
div p:first-child /*FOR FIRST PARA IN DIV*/
{
    background-color: yellow;
    color: red;
}
.A{
    color: blue;
}
.B{
    color: brown !important;/*IMPORTANT COLOR APPLYING*/
}
.C{
    color: aqua
}


